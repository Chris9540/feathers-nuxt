{
  "name": "feathers-nuxt",
  "description": "A Feathers + Nuxt demo/boilerplate",
  "version": "0.0.0",
  "homepage": "https://github.com/silvestreh/feathers-nuxt",
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Silvestre Herrera",
    "email": "silvestre.herrera@gmail.com"
  },
  "repository": "silvestreh/feathers-nuxt",
  "license": "MIT",
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^10.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "build": "nuxt build",
    "coverage": "cross-env NODE_ENV=test nyc npm run mocha",
    "test": "npm run eslint && npm run coverage",
    "eslint": "eslint src/. test/. --config .eslintrc.json",
    "dev": "cross-env DEBUG=nuxt:* nodemon src/",
    "start": "npm run build && node src/",
    "mocha": "cross-env NODE_ENV=test mocha test/ --recursive --exit"
  },
  "dependencies": {
    "@feathersjs/adapter-commons": "^4.3.8",
    "@feathersjs/authentication": "^4.3.7",
    "@feathersjs/authentication-client": "^4.3.7",
    "@feathersjs/authentication-jwt": "^2.0.10",
    "@feathersjs/authentication-local": "^4.3.7",
    "@feathersjs/authentication-oauth": "^4.3.7",
    "@feathersjs/commons": "^4.3.7",
    "@feathersjs/configuration": "^4.3.7",
    "@feathersjs/errors": "^4.3.7",
    "@feathersjs/express": "^4.3.7",
    "@feathersjs/feathers": "^4.3.7",
    "@feathersjs/socketio": "^4.3.7",
    "@feathersjs/socketio-client": "^4.3.7",
    "compression": "^1.7.4",
    "cookie-storage": "^5.0.3",
    "cors": "^2.8.5",
    "feathers-nedb": "^5.1.0",
    "feathers-vuex": "^2.0.4",
    "helmet": "^3.21.2",
    "nedb": "^1.8.0",
    "normalize.css": "^8.0.1",
    "nuxt": "^2.10.2",
    "serve-favicon": "^2.5.0",
    "socket.io-client": "^2.3.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "cross-env": "^6.0.3",
    "eslint": "^6.5.1",
    "mocha": "^6.2.2",
    "nodemon": "^1.19.4",
    "nyc": "^14.1.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.4"
  }
}
